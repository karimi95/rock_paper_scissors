<!DOCTYPE html>
<html>
  <head>
    <title>Rock Paper Scissors</title>
  </head>

  <script>
    let compChoice;
    
    function computerPlay(){

    let num = Math.floor(Math.random() * 10);
    if(num >= 0 || num <= 2){
			  compChoice = "Rock";
			  }
			  else if(num >= 3 || num <= 5){
						     compChoice = "Paper";
						     }
						     else if(num >= 6 || num <= 8){
										compChoice = "Scissors";
										}
										else
										computerPlay();
										

    }

  </script>





</html>
